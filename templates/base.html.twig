                    {% endif %}

                    {% if app.user %}
                        <h6 style="right:5%;position: absolute;">
                            Bonjour {{ app.user.login }} !
                            <a href="{{ path('user_details', { id: app.user.id }) }}" class="link-dark" style="text-decoration: none !important;">

                                <svg xmlns="http://www.w3.org/2000/svg" height="1.75em" viewBox="0 0 448 512">
                                    <style>
                                        svg{fill:#000000}
                                    </style>
                                    <path d="M224 256A128 128 0 1 0 224 0a128 128 0 1 0 0 256zm-45.7 48C79.8 304 0 383.8 0 482.3C0 498.7 13.3 512 29.7 512H418.3c16.4 0 29.7-13.3 29.7-29.7C448 383.8 368.2 304 269.7 304H178.3z"/>
                                </svg>
                            </a>
                        </h6>
                        {% endif %}
                </ul>
            </div>
        </nav>
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
